<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="Clock_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Learning_Fragments">
        <activity android:name="com.learning.Clock_app.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name="com.learning.Clock_app.NotificationReceiver"
            tools:ignore="Instantiatable" />

        <activity android:name="com.learning.Clock_app.ActivityNewAlarm"
            android:parentActivityName="com.learning.Clock_app.MainActivity"/>
    </application>

</manifest>